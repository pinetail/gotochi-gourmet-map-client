<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
  android:id="@+id/RelativeLayout01"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content">
    <LinearLayout android:layout_width="fill_parent"
     android:gravity="center" android:id="@+id/header_ad" android:layout_height="wrap_content" android:layout_alignParentTop="true">
        <jp.Adlantis.Android.AdlantisView android:id="@+id/adView" android:layout_height="50dp" android:layout_width="fill_parent" android:layout_weight="1" android:gravity="center"></jp.Adlantis.Android.AdlantisView>
    </LinearLayout>
  <LinearLayout
    android:id="@+id/LinearLayout03"
    android:layout_below="@+id/header_ad"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <ListView android:id="@+id/savedList"
      android:layout_width="fill_parent"
      android:layout_height="wrap_content" />
  </LinearLayout>
  <LinearLayout android:id="@+id/linearLayout1"
 android:layout_width="fill_parent" android:gravity="bottom|right" android:layout_height="wrap_content" android:layout_alignParentBottom="true">
      <ImageView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/imageView1" android:src="@drawable/tabelog_api_135x17"></ImageView>
  </LinearLayout>




</RelativeLayout>

